{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-trailing-spaces": ["error", {"skipBlankLines": true}],
        "indent": "error",
        "semi": "error",
        "no-extra-semi": "error",
        "semi-spacing": "error"

    }
}
